Host lis
    User eloi.perdereau
    Hostname saphir2.lis-lab.fr
    IdentityFile ~/.ssh/lis_rsa

Host gitlab-lis
    Hostname gitlab.lis-lab.fr
    IdentityFile ~/.ssh/lis_rsa
    IdentitiesOnly yes

Host gitlab-inria
    Hostname gitlab.inria.fr
    IdentityFile ~/.ssh/id_ed25519
    PreferredAuthentications publickey

Host github
    Hostname github.com
    IdentityFile ~/.ssh/id_ed25519
    IdentitiesOnly yes

Host g5k
    User eperdereau
    IdentityFile ~/.ssh/imt_rsa
    Hostname access.grid5000.fr
    ForwardAgent no
    IdentitiesOnly yes

Host *.g5k
    User eperdereau
    IdentityFile ~/.ssh/imt_rsa
    ProxyCommand ssh g5k -W "$(basename %h .g5k):%p"
    ForwardAgent no
    IdentitiesOnly yes
